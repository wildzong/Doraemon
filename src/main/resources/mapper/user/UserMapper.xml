<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doraemon.lottery.login.dao.UserDao">
    <resultMap id="user_entityResultMap" type="com.doraemon.lottery.login.entity.UserEntity">
        <id column="id" property="id" />
        <result column="user_name" property="userName" />
    </resultMap>

    <select id="getUserByUsername" resultMap="user_entityResultMap">
        select id, user_name from t_user where user_name = #{userName}
    </select>
</mapper>